{
    "manifest_version": 2,

    "name": "Silence of the Tabs",
    "description": "Unload Tabs. Focus on restorability after crashes, addon-deletion and on synchronized devices.",
    "version": "0.1",

    "browser_action": {
        "default_popup": "main.html#popuppage"
    },
    "options_page": "main.html#optionspage", 
    "background": {
        "page": "main.html#backgroundpage"
    },

    "permissions": [
        "tabs", "storage"
    ]
}
